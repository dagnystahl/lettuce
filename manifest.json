{
    "manifest_version": 2,
    "name": "Lettuce",
    "version": "0.1",
    "description": "Converts all scary meat references to friendly happy vegetables.",
    "icons": { "16": "lettuce16.png",
               "32": "lettuce32.png",
              "128": "lettuce128.png" },
    "content_scripts": [
        {
                        "matches": ["<all_urls>"],
                        "js": ["content.js"]
        }
    ]
}
